{
  "name": "@sakuraapi/api",
  "version": "0.8.0",
  "description": "MongoDB and TypeScript MEAN Stack Framework for NodeJS",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/src/**/!(*.spec).js",
    "lib/src/**/!(*.spec).d.ts",
    "lib/index.*"
  ],
  "scripts": {
    "build": "./scripts/build.sh",
    "coverage": "./scripts/coverage.sh",
    "doc:generate": "./scripts/doc:generate.sh",
    "doc:serve": "./scripts/doc:serve.sh",
    "docker:compose-test": "./scripts/docker:compose-test.sh",
    "install": "chmod +x scripts/*.* || true",
    "lint": "./scripts/lint.sh",
    "prepublish": "npm test && npx nsp check",
    "say:fail": "say 'fail' || echo 'fail'",
    "say:pass": "say 'pass' || echo 'pass'",
    "start": "nodemon --exec \"npm run build; echo build done\"",
    "start:test": "nodemon --exec \"npm test\"",
    "test": "./scripts/test.sh",
    "test:db": "./scripts/test.sh saveDb",
    "test:debug": "DEBUG=sapi:*,-sapi:*:verbose npm test",
    "test:verbose": "DEBUG=sapi:* npm test",
    "test:vverbose": "DEBUG=* npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sakuraapi/api.git"
  },
  "keywords": [
    "REST",
    "JSON",
    "api",
    "server"
  ],
  "author": "Jean-Pierre E. Poveda",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/sakuraapi/api/issues"
  },
  "homepage": "https://github.com/sakuraapi/api#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@types/colors": "^1.1.3",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.0",
    "@types/jasmine": "2.5.45",
    "@types/lodash": "^4.14.92",
    "@types/mongodb": "^2.2.18",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "body-parser": "^1.18.2",
    "helmet": "^3.9.0",
    "http-server": "^0.10.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nodemon": "^1.14.9",
    "nsp": "^3.1.0",
    "source-map-support": "^0.5.0",
    "supertest": "^3.0.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "bson-ext": "^1.0.5",
    "co": "^4.6.0",
    "colors": "^1.1.2",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.33",
    "reflect-metadata": "^0.1.10",
    "uuid": "^3.1.0"
  }
}
